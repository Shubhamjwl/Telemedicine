.error {
  color: red;
}

.container {
  border: 1px solid black;
  margin: 60px auto;
  width: 950px;
  height: 545px;
  background-color: white;
  margin-top: 0px;
}
.nav-container {
  width: 950px;
  height: 55px;
  background-color: #e43927;
  margin-left: -16px;
}
.nav-text {
  color: white;
  margin-left: 20px;
  padding-top: 16px;
  font-size: 20px;
}
.main-text {
  font-size: 22px;
  font-family: Poppins-Regular sans-serif;
  padding-left: 70px;
  padding-top: 70px;
}
.text {
  font-size: 20px;
  font-family: Poppins-Regular, sans-serif;
  padding-left: 70px;
}
.input-text {
  font-size: 20px;
  font-family: Poppins-Regular, sans-serif;
  color: black;
}
.input-box {
  display: block;
  width: 100%;
  height: calc(1.5em + 0.75rem + 2px);
  padding: 0.375rem 0.75rem;
  font-size: 0.875rem;
  font-weight: 400;
  line-height: 1.5;
  color: #495057;
  background-color: #fff;
  background-clip: padding-box;
  border: 0px;
  transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
}

.button {
  width: 131px;
  height: 42px;
  color: #fff;
  background: #e43927;
  border-radius: 50px;
  font-size: 15px;
  text-transform: uppercase;
  font-family: Poppins-Regular, sans-serif;
  font-weight: 600;
  cursor: pointer;
  border: 2px solid #e43927;
  transition: 0.5s;
  letter-spacing: 1.2px;
}
.logo-img {
  width: 190px;
  margin-left: 17%;
  margin-top: -6%;
}
