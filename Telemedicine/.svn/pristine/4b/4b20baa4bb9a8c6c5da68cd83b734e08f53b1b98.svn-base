  border-bottom: 0px;
}

.User-Dropdown > li > a {
  font-size: 15px;
  padding: 0;
  text-decoration: none;
  color: #000;
  transition: all 0.2s ease-out;
  font-weight: 500;
  text-align: center;
  display: inline-block;
}

