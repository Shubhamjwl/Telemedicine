.tool {
  cursor: pointer;
  font-size: 12px;
  position: relative;
  display: inline-block;
  margin: 0;
}

/* st = Style tol tip */
.tool span {
  opacity: 0;
  color: #fff;
  width: 55px;
  padding: 5px 0;
  background: #000000;
  text-align: center;
  position: absolute;
  border-radius: 6px;
  transition: opacity 1s;
}
.tool span:after {
  content: "";
  border-width: 5px;
  position: absolute;
  border-style: solid;
}
.tool:hover span {
  opacity: 1;
}

.B-st {
  left: 3px;
  bottom: -15px;
}
.B-st:after {
  right: 45%;
  bottom: 100%;
  border-color: transparent transparent #000000 transparent;
}

span.B-st.st2 {
  width: 119px;
  left: -33px;
}

span.B-st.st1 {
  width: 80px;
  left: -9px;
}
