}

.input-group input[type="text"],
.input-group input[type="password"] {
  width: 100%;
  border: 0;
  border-bottom: 1px solid #70707075;
  float: left;
  // height: 24px;
  outline: 0;
  padding-left: 36px;
  padding-bottom: 20px;
  margin-top: 16px;
}

.input-group input[type="email"] {
  width: 100%;
  border: 0;
  border-bottom: 1px solid #70707075;
  float: left;
  // height: 24px;
  outline: 0;
  padding-left: 36px;
  padding-bottom: 20px;
  margin-top: 16px;
}

.input-group input.uname {
