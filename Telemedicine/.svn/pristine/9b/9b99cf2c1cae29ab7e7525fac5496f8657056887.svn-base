  margin-right: 0px;
  margin-bottom: 40px;
}

.right-nav{
  float: left;
  padding-top: 15px;
  margin-left: 500px;
}

.login-form {
  float: right;
  width: 570px;
}

.textJustify{
    text-align: justify;
    text-justify: inter-word;
    font-family:FontAwesome;
}

.input-group label {
  font-size: 15px;
  font-weight: bold;
  width: 100%;
  float: left;
  letter-spacing: 0.6px;
}

.input-group {
