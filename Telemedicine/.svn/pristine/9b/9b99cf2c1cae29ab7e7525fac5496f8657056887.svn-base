  padding: 0 0 25px 0;
  list-style: none;
  width: 48%;
}

span.box-text {
  font-size: 15px;
  float: left;
  margin-left: 10px;
  margin-top: 8px;
  line-height: 17px;
  letter-spacing: 0.6px;
}

span.box-icon {
