$asset-image-path: "~src/assets/img";
a:hover {
  color: #000 !important;
}



input {
  border-radius: 0 !important;
  font-family: "Roboto", sans-serif;
}


input,
select,
textarea {
  border-radius: 0 !important;
  font-family: "Roboto", sans-serif;
}

.conatiner-fluid {
  width: 100%;
  margin: 0 auto;
  padding: 0px;
  font: normal normal normal 14px / normal "Roboto", sans-serif !important;
  background-color: white;
  color: #000;
}

.login-conatiner {
  position: relative;
  float: left;
}

.login-conatiner::before {
  background: url("#{$asset-image-path}/image-red.png") no-repeat left top;
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 479px;
  height: 323px;
}

.login-conatiner::after {
  background: url("#{$asset-image-path}/reg-bottom-shape.png") no-repeat left
    top;
  content: "";
  position: absolute;
  right: 0;
  bottom: -13px;
  width: 257px;
  height: 125px;
}

.left-area {
  width: 45%;
  margin-top: 340px;
  float: left;
  padding-left: 50px;
}

.right-area {
  width: 44%;
  float: right;
  padding-right: 80px;
}

.tagline {
  font-size: 30px;
  font-weight: bold;
  letter-spacing: 1.2px;
}

p.text-primary.mt-3.tagline2 {
