package com.nsdl.otpManager.enumeration;



public enum TemplateType {
	
	FORGOT,
	REG,
	CHANGE,
	VERIFY,
	REJECT,
	APPOINTMENT_DTLS_DOC,
	APPOINTMENT_DTLS_PT,
	APPOINTMENT_CANCEL_DOC,
	APPOINTMENT_CANCEL_PT,
	APPOINTMENT_RESCHEDULE_DOC,
	APPOINTMENT_RESCHEDULE_PT,
	CONSULTATION_SUCCESS_DOC,
	CONSULTATION_SUCCESS_PT
	;
	
	 public String templateTypeName() {
	        switch (this) {
	            case FORGOT:
	                return "Forgot_Password_";
	            case REG:
	                return "Registration_Success_";
	            case CHANGE:
	                return "Change_Password_";
	            case VERIFY:
	                return "Doctor_Verification_";
	            case REJECT:
	                return "Doctor_Verification_Fail_";
	            case APPOINTMENT_DTLS_DOC:
	                return "Appointment_Dtls_Doctor_";
	            case APPOINTMENT_DTLS_PT:
	                return "Appointment_Dtls_Patient_";
	            case APPOINTMENT_CANCEL_DOC:
	            	return "Appointment_Cancellation_Doctor_";
	            case APPOINTMENT_CANCEL_PT:
	            	return "Appointment_Cancellation_Patient_";
	            case APPOINTMENT_RESCHEDULE_DOC:
	            	return "Reschedule_Dtls_Doctor_";
	            case APPOINTMENT_RESCHEDULE_PT:
	            	return "Reschedule_Dtls_Patient_";
	            case CONSULTATION_SUCCESS_DOC:
	            	return "Consultation_Success_Doctor_";
	            case CONSULTATION_SUCCESS_PT:
	            	return "Consultation_Success_Patient_";
	           	            	
	            default:
	                return null;
	        }
	    }

}
