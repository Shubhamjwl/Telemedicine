            <div class="form-group">
              <input
                [type]="hidePassword ? 'password' : 'text'"
                [(ngModel)]="password"
                name="pass"
                autocomplete="disabled"
                class="input-border mr-2 input"
                placeholder="Enter Password"
                #pass="ngModel"
                style="width: 165%; border-bottom: 1px solid #e43927 !important"
                pattern="^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$%^&*-]).{8,}$"
                minlength="8"
                required
              />
              <i
                class="fa"
                [ngClass]="{
                  'fa-eye': !hidePassword,
                  'fa-eye-slash': hidePassword
                }"
                aria-hidden="true"
                (click)="hide()"
              ></i>
            </div>
            <div
              *ngIf="pass.invalid && (pass.dirty || pass.touched)"
              class="error"
            >
              <div *ngIf="pass.errors.required">Password is required</div>
              <div *ngIf="pass.errors.pattern">
                Password must contain uppercase, lowercase, special character &
                number.
              </div>
            </div>
          </div>
          <div class="col-md-3 lable" *ngIf="!isShownAfterRegistration">
