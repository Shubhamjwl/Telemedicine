      <div class="row" style="margin-top: -5%">
        <div class="col-3 col-lg-3 lable" *ngIf="!isShownAfterRegistration">
          <p class="input-text" style="margin-top: 16%">
            Password<span class="error">*</span>
          </p>
        </div>
        <div class="col-5 col-lg-5" *ngIf="!isShownAfterRegistration">
          <div class="form-group">
            <input
              [type]="hidePassword ? 'password' : 'text'"
              [(ngModel)]="password"
              name="pass"
              autocomplete="disabled"
              class="input-border mr-2 input"
              placeholder="Enter Password"
              #pass="ngModel"
              style="width: 130%; border-bottom: 1px solid #e43927 !important"
              pattern="^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$%^&*-])(?!.*((12)|(23)|(34)|(45)|(56)|(67)|(78)|(90)|(01))).{8,}$"
              minlength="8"
              required
            />

            <div
              *ngIf="pass.invalid && (pass.dirty || pass.touched)"
              class="error"
            >
              <div *ngIf="pass.errors.required">Password is required</div>
              <div *ngIf="pass.errors.pattern">
                Password must have a minimum of 8 characters and contain
                uppercase, lowercase, special character, number & doesn't allow
                sequential numbers like 123..
              </div>
            </div>
          </div>
        </div>
        <div
          class="col-2 col-lg-2 lable"
          *ngIf="!isShownAfterRegistration"
          style="margin-top: 2%; margin-left: 10%"
        >
          <i
            class="fa"
            [ngClass]="{
              'fa-eye': !hidePassword,
              'fa-eye-slash': hidePassword
            }"
            aria-hidden="true"
            (click)="hide()"
          ></i>
        </div>
      </div>

      <div class="row" style="margin-top: -5%">
