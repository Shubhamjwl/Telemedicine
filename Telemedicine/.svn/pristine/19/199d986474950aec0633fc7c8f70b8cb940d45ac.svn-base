<div
  *ngIf="isShownRegistration"
  class="sample form-group"
  style="background-color: white; padding-left: 3%"
>
  <nav class="navbar navbar-expand-sm navbar-dark" style="margin-left: -5%">
    <p class="nav-text">Create ABHA ID</p>
  </nav>

  <div style="margin-left: 2%">
    <form
      #regestrationForm="ngForm"
      novalidate
      autocomplete="off"
      style="width: 100%"
    >
      <div class="row">
        <div class="col-3 col-lg-3">
          <p
            class="input-text"
            *ngIf="isShownRegistration"
            style="margin-top: 15%"
          >
            Mobile Number<span class="error">*</span>
          </p>
          <p class="input-text" *ngIf="!isShownRegistration">
            Enter Mobile Number<span class="error">*</span>
          </p>
        </div>
        <div class="col-8 col-lg-8">
          <div class="form-group">
            <input
              type="text"
              name="mobilePrifix"
              [readonly]="isMobileNoDisabled"
              class="input-border mr-2 mobile-pre"
              placeholder="+91"
              style="
                display: inline-block;
                border-bottom: 1px solid #e43927 !important;
              "
            />
            <input
              type="text"
              name="mobileNo"
              #mobileNo="ngModel"
              [(ngModel)]="mobileNoSel"
              [readonly]="isMobileNoDisabled"
              maxlength="10"
              pattern="{{ PATTERN.MOBILE_NUMBER }}"
              class="input-border input mobile-post"
              placeholder="Mobile Number"
              style="
                display: inline-block;
                border-bottom: 1px solid #e43927 !important;
              "
              required
            />
          </div>
        </div>
      </div>

      <div class="row" style="margin-top: -3%">
        <div class="col-3 col-lg-3">
          <p class="input-text" style="margin-top: 7%">
            Name<span class="error">*</span>
          </p>
        </div>
        <div class="col-8 col-lg-8">
          <div class="form-group">
            <input
              type="text"
              [(ngModel)]="lastName"
              [readonly]="isShownAfterRegistration"
              class="input-border input"
              name="lname"
              autocomplete="disabled"
              placeholder="Last Name"
              style="
                width: 26%;
                display: inline-block;
                border-bottom: 1px solid #e43927 !important;
              "
            />
            <input
              type="text"
              [(ngModel)]="firstName"
              [readonly]="isShownAfterRegistration"
              name="fname"
              autocomplete="disabled"
              #fname="ngModel"
              class="input-border mr-1 input"
              placeholder="First Name"
              required
              style="
                width: 26%;
                display: inline-block;
                margin-left: 15px;
                border-bottom: 1px solid #e43927 !important;
              "
            />
            <input
              type="text"
              [(ngModel)]="middleName"
              [readonly]="isShownAfterRegistration"
              name="mname"
              autocomplete="disabled"
              class="input-border mr-1 input"
              placeholder="Middle Name"
              style="
                width: 27%;
                display: inline-block;
                margin-left: 15px;
                border-bottom: 1px solid #e43927 !important;
              "
            />
            <div class="error" [hidden]="fname.valid || fname.untouched">
              Enter Valid Name
            </div>
          </div>
        </div>
      </div>

      <div class="row" style="margin-top: -5%">
        <div class="col-3 col-lg-3">
          <p class="input-text" style="margin-top: 16%">
            Date of Birth<span class="error">*</span>
          </p>
        </div>
        <div class="col-8 col-lg-8">
          <div class="form-group">
