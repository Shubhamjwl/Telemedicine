.error {
  color: red;
}
.container {
  border: 1px solid black;
  margin: 60px auto;
  width: 1050px;
  height: 720px;
  background-color: white;
  margin-top: 0px;
}

.nav-container {
  width: 1048px;
  height: 55px;
  background-color: #e43927;
  margin-left: -16px;
}
.nav-text {
  color: white;
  margin-left: 20px;
  padding-top: 16px;
  font-size: 20px;
}
.main-text {
  font-size: 22px;
  font-family: "Poppins-Regular, sans-serif";
  padding-left: 70px;
  padding-top: 70px;
}
.text {
  font-size: 20px;
  font-family: "Poppins-Regular, sans-serif";
  padding-left: 70px;
}
.input-box {
  display: block;
  width: 100%;
  height: calc(1.5em + 0.75rem + 2px);
  padding: 0.375rem 0.75rem;
  font-size: 1rem;
  font-weight: 400;
  line-height: 1.5;
  color: #495057;
  background-color: #fff;
  background-clip: padding-box;
  border: 0px;
  /* border-radius: 0.25rem; */
  transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
}

.button {
  color: #fff;
  border-color: #e43927;
  border-radius: 35px;
  background-color: #e43927;
  height: 58px;
  width: 156px;
  font-weight: bold;
  font-size: 0.875rem;
  font-family: Poppins-Regular, sans-serif;
  letter-spacing: 1.2px;
  white-space: normal;
}
.logo-img {
  width: 190px;
  margin-left: 17%;
  margin-top: -6%;
}
