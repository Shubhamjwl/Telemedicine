.tool {
  cursor: pointer;
  font-size: 12px;
  position: relative;
  display: inline-block;
  margin: 0;
}

/* st = Style tol tip */
.tool span {
  opacity: 0;
  color: #fff;
  width: 55px;
  padding: 5px 0;
  background: #000000;
  text-align: center;
  position: absolute;
  border-radius: 6px;
  transition: opacity 1s;
}
.tool span:after {
  content: "";
  border-width: 5px;
  position: absolute;
  border-style: solid;
}
.tool:hover span {
  opacity: 1;
}

