.home-section {
  position: relative;
  background: #e4e9f7;
  height: 100vh;
  left: 260px;
  width: calc(100% - 260px);
  transition: all 0.5s ease;
}

.sidebar.close ~ .home-section {
  left: 78px;
  width: calc(100% - 78px);
}

.home-section .home-content {
  height: 60px;
  display: flex;
  align-items: center;
}

.home-section .home-content .bx-menu,
.home-section .home-content .text {
  color: #11101d;
  font-size: 35px;
}

.home-section .home-content .bx-menu {
  margin: 0 15px;
  cursor: pointer;
}

.home-section .home-content .text {
  font-size: 26px;
  font-weight: 600;
}

.User-area {
  width: 45px;
  height: 45px;
  position: relative;
  cursor: pointer;
}

.User-area > .User-avtar {
  width: 41px;
  height: 41px;
  border-radius: 30px;
  /* box-shadow: 0px 3px 40px #00000029; */
  color: #fff;
  text-align: center;
  // background: url(../images/doctor-small-photo.png) no-repeat center top;
  line-height: 42px;
  font-size: 17px;
}

.User-area > .User-avtar > img {
  width: 100%;
  height: 100%;
  border-radius: 30px;
}

.User-Dropdown {
  display: none;
  position: absolute;
  border-radius: 0;
  list-style: none;
  padding-top: 0;
  min-width: 250px;
  margin: 0;
  top: 100%;
  right: 7%;
  z-index: 99;
  text-align: center;
}

.User-Dropdown .User-Dropdown-inside {
  background: #fff;
  -webkit-box-shadow: 0 2px 10px rgb(0 0 0 / 20%);
  box-shadow: 0px 3px 15px #00000029;
  padding: 30px 46px;
  width: 187px;
  height: 300px;
  border-radius: 20px;
}

.User-Dropdown:before {
  /*  content: ""; */
  position: absolute;
  width: 0;
  height: 0;
  margin-left: -0.5em;
  right: 0px;
  box-sizing: border-box;
  border: 7px solid black;
  border-color: transparent transparent #fff #fff;
  transform-origin: 0 0;
  transform: rotate(135deg);
  box-shadow: -3px 3px 3px -3px rgb(214 214 214 / 78%);
  top: 0;
}

.User-Dropdown.U-open {
  display: block !important;
  z-index: 9999;
}

.User-area:hover .User-Dropdown {
  display: block;
}

.User-Dropdown > li {
  padding: 0px;
  line-height: 47px;
  border-bottom: 1px solid rgba(215, 215, 215, 0.17);
  text-align: center;
}

.User-Dropdown > li:last-child {
  border-bottom: 0px;
}

.User-Dropdown > li > a {
  font-size: 15px;
  padding: 0;
  text-decoration: none;
  color: #000;
  transition: all 0.2s ease-out;
  font-weight: 500;
  text-align: center;
  display: inline-block;
}

.User-Dropdown span {
  background: #16d67a;
  padding: 3px 10px;
  color: #fff;
  border-radius: 30px;
  font-size: 13px;
}

.User-Dropdown > li a:hover {
  color: #1b5ade;
}

.profile-img .User-avtar > img {
  width: 80px;
}

.profile-img .User-avtar {
  position: relative;
  width: auto;
  display: inline-block;
}

.change-profile-img {
  position: absolute;
  background: #fff;
  bottom: 0;
  position: absolute;
  right: 0;
  overflow: visible;
  height: 32px;
  width: 32px;
  border-radius: 50%;
}

.change-profile-img a {
  box-shadow: 0 1px 1px 0 rgb(65 69 73 / 30%), 0 1px 3px 1px rgb(65 69 73 / 15%);
  margin: 0px 2.5px 3px;
  -webkit-box-shadow: 0 1px 1px 0 rgb(65 69 73 / 30%),
    0 1px 3px 1px rgb(65 69 73 / 15%);
  outline: 0;
  position: absolute;
  right: 0;
  height: 26px;
  width: 26px;
  bottom: 0;
  border-radius: 50%;
}

.User-name {
  color: #000;
  font-weight: 500;
  margin-top: 10px;
  font-size: 16px;
}

.User-logout a {
  background-color: #ffffff;
  border: 1px solid #dadce0;
  -webkit-border-radius: 100px;
  border-radius: 100px;
  color: #000;
  display: inline-block;
  font-weight: 400;
  letter-spacing: 0.25px;
  margin: 14px 0 0;
  max-width: 254px;
  outline: 0;
  padding: 3px 16px;
  text-align: center;
  text-decoration: none;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 14px;
  border-radius: 4px;
  transition: 0.5s;
}

.User-logout a:hover {
  background-color: #1b5ade;
  color: #fff;
}

.doc-name {
  font-size: 13px;
  color: #77797a;
  letter-spacing: 0.78px;
