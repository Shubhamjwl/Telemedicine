.overlay {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.7);
  transition: opacity 500ms;
  visibility: hidden;
  opacity: 0;
  z-index: 999999;
}
.overlay:target {
  visibility: visible;
  opacity: 1;
  z-index: 9999999;
}

.popup {
  // margin: 25px auto 0 auto;
  padding: 0 !important;
  background: #fff;
  border-radius: 0 !important;
  width: 756px;
  position: relative;
  transition: all 5s ease-in-out;
}

.popup2 {
  position: relative;
}

#popup2 .popup {
  margin-top: 40px !important;
  width: 693px;
  padding-top: 0 !important;
  padding-bottom: 25px !important;
}

#popup3 .popup {
  margin-top: 40px !important;
  width: 693px;
  padding-top: 0 !important;
  padding-bottom: 25px !important;
}

.popup2 .close {
  display: inline-block;
  position: absolute;
  top: -3px;
  text-align: center;
  right: 0;
  font-weight: bold;
  text-decoration: none;
  background: #a92a1d;
  line-height: 10px;
  width: 55px;
  height: 38px;
  padding: 9px;
}

.item:hover .date,
.item:hover .day {
  color: #fff;
}
.item {
  transition: 0.5s;
}
.item:hover {
  background: #e43927 !important;
}

input[type="date"]::-webkit-inner-spin-button {
  -webkit-appearance: none;
}

@media screen and (max-width: 700px) {
  .box {
    width: 70%;
  }
  .popup {
    width: 70%;
  }
}

