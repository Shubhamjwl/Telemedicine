:host {
  display: flex;
  background-color: white;
}

input {
  border-radius: 0 !important;
  font-family: "Roboto", sans-serif;
}

input,
select,
textarea {
  border-radius: 0 !important;
  font-family: "Roboto", sans-serif;
}

.content {
  margin-left: 80px;
  margin-top: 100px;
  max-width: 800px;
}

.header-panel {
  background: #fff;
  color: #0069d9;
  height: 84px;
  position: fixed;
  width: 100%;
  z-index: 999999;
  box-shadow: 1px 6px 11px -8px rgb(14 77 146 / 15%);
  -webkit-box-shadow: 1px 2px 8px 0px rgb(14 77 146 / 15%);
}

.welcome-user {
  float: right;
}

.moth-slot {
  float: left;
  width: 100%;
  margin-top: 20px;

  ul {
    margin: 0;
    padding: 0;

    li {
      list-style: none;
      margin-right: 10px;
      float: left;
      margin-bottom: 10px;

      a {
        width: 39px;
        height: 39px;
        font-size: 15px;
        color: #000;
        border: 1px solid #e43927;
        border-radius: 50px;
        float: left;
        text-align: center;
        line-height: 38px;
        text-decoration: none;
        transition: 0.5s;
        display: flex;
        justify-content: center;
        align-items: center;

        &:hover,
        &.selected {
          color: #fff !important;
          border: 1px solid #e43927;
          background: #e43927;
        }
      }
    }
  }
}

.days-area {
  float: left;
  width: 100%;
  margin-top: 20px;

  ul {
    margin: 0;
    padding: 0;

    li {
      list-style: none;
      margin-right: 17px;
      float: left;

      a {
        width: -webkit-fill-available;
        height: auto;
        font-size: 15px;
        color: #000;
        border: 1px solid #e43927;
        border-radius: 6px;
        float: left;
        text-align: center;
        line-height: 19px;
        text-decoration: none;
        padding: 9px 15px;
        transition: 0.5s;
        text-transform: uppercase;

        &:hover,
        &.selected {
          color: #fff !important;
          border: 1px solid #e43927;
          background: #e43927;
        }
      }
    }
  }
}

.header-left {
  float: right;
  margin-left: 25px;
  margin-top: 10px;
}

.header-left.header-left2 {
  margin-left: 59px;
  padding-top: 0;
  float: left;
  margin-top: 7px;
}

.header-right {
  float: right;
  width: 110px;
  margin-right: 50px;
  padding-top: 21px;
  margin-left: 43px;
}

.sidebar {
  position: fixed;
  top: 84px;
  left: 0;
  height: 88%;
  width: 197px;
  padding-right: 50px;
  background: #fff;
  z-index: 99999;
  border-right: 1px solid #eee;
  transition: all 0.5s ease;
}

.sidebar.close {
  width: 65px;
  padding-right: 0;
}

.sidebar.close .nav-links li a:hover {
  width: 200px;
  background: #2d4294;
}

.sidebar.close .nav-links li a:hover span.link_name {
  opacity: 1;
  color: #fff;
}

.notify-area {
  width: 30px;
  float: right;
  position: relative;
}

.sidebar .nav-links li {
  position: relative;
  list-style: none;
}

.sidebar .nav-links li .iocn-link {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.sidebar.close .nav-links li .iocn-link {
  display: block;
}

.sidebar .nav-links li i {
  height: 50px;
  min-width: 78px;
  text-align: center;
  line-height: 50px;
  color: #fff;
  font-size: 20px;
  cursor: pointer;
}

.sidebar .nav-links li.showMenu i.arrow {
  transform: rotate(-180deg);
}

.sidebar.close .nav-links i.arrow {
  display: none;
}

.sidebar .nav-links li a {
  display: flex;
  align-items: center;
  text-decoration: none;
  padding: 17px 0 17px 45px;
  margin-left: 0;
  width: 100%;
  border-top: 1px solid #e4e4e4;
}

.sidebar.close .nav-links li a {
  width: 30%;
}

.sidebar .nav-links li a .link_name {
  font-size: 15px;
  font-weight: 500;
  line-height: 30px;
  margin-left: 10px;
  color: #000;
}

