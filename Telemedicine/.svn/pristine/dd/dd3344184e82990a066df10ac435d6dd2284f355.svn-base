.repeat-area input[type="checkbox"] {
  display: none;
}

.repeat-area input[type="checkbox"] + label:before {
  text-align: center;
  content: "\2714";
  border: 1px solid #e0e0e0;
  border-radius: 0.2em;
  display: inline-block;
  width: 17px;
  height: 17px;
  padding-left: 0;
  padding-bottom: 0;
  margin-right: 10px;
  vertical-align: bottom;
  color: transparent;
  transition: 0.2s;
}

.repeat-area input[type="checkbox"] + label:active:before {
  transform: scale(0);
}

.repeat-area input[type="checkbox"]:checked + label:before {
  background-color: #e43927;
  border-color: #e43927;
  color: #fff;
}

.repeat-area input[type="checkbox"]:disabled + label:before {
  transform: scale(1);
  border-color: #aaa;
}

.repeat-area input[type="checkbox"]:checked:disabled + label:before {
  transform: scale(1);
  background-color: #f7c28f;
  border-color: #f7c28f;
}

