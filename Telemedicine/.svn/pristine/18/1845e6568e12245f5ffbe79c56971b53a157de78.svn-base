/**
 * regex used in app
 *
 * @param
 * @returns
 */
export let REGEX = {
  OTP: '^[0-9]{6,6}$',
  Aadhaar: '^[0-9]{12,12}$',
  NAME: "(?![0-9\\s+]*$)[a-zA-Z\\s\\-_.']+",
  ALPHA_NUMERIC:
    "(?![0-9\\s+]*$)[a-zA-Z0-9\\s\\-_\\.'@#%\\&\\!\\$\\^\\*\\(\\)\\[\\]\\{\\}\\:;,]+",
  NAME_SPL: '^[A-Z]{1,10}$',
  ONLY_ALPHABATES: '(?![0-9\\s+]*$)[a-zA-Z\\s&]+',
  NEW_NAME: "^[a-zA-Z']+( [a-zA-Z']+)*$",
  NAME_NUMBER: "(?![0-9\\s+]*$)[a-zA-Z\\s\\-_.'0-9]+",
  // ADDRESS: "(?![0-9\\s+]*$)[a-zA-Z\\s-_.,'0-9@#$%^&\\*\\(\\)\\{\\}\\[\\]]+",
  ADDRESS: "(?![0-9\\s+]*$)[a-zA-Z\\s_.,'0-9\\-]+",
  PINCODE: '^[1-9][0-9]{5}$',
  ONLY_NUMBER: '^[0-9]*$',
  GST: '^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$',
  PAN: '^([a-zA-Z]){5}([0-9]){4}([a-zA-Z]){1}?$',
  // PAN: "^[A-Za-z]{5}\d{4}[A-Za-z]{1}$",
  EMAIL: '\\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,4}\\b',
  DECIMAL_NOT_ZERO: '^([0-9]*[1-9][0-9]*(.[0-9]+)?|[0]+.[0-9]*[1-9][0-9]*)$',
  NOT_ZERO: '^([0-9]*[1-9][0-9]*(.[0-9]+)?|[0]+.[0-9]*[1-9][0-9]*)$',
  MOBILE_NUMBER: '^[6-9]\\d{9}$',
  PHONE_NUMBER:
    '(?:\\s+|)((0|(?:(\\+|)91))(?:(?:\\d(?:\\s|-)*\\d{9})|(?:\\d{2}(?:\\s|-)*\\d{8})|(?:\\d{3}(?:\\s|-)*\\d{7}))|\\d{10})(?:\\s+|)',
