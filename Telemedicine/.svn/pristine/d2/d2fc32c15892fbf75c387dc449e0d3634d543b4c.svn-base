@media only screen and (min-width: 768px) {
  /* For mobile phones: */
  [class*="col-"] {
    width: 100%;
    height: 100%;
  }
  .sample {
    border: 1px solid black;
    margin-left: 25%;
    width: 50%;
  }
}

@media screen and (min-width: 768px) {
  .form-groupBlogg {
    width: 100%;
    margin-top: 6%;
    margin-bottom: 2%;
  }
  /* .form-group {
    height: 113%;
  } */
  .input-line {
    width: 100%;
  }
  .mobile-input {
    width: 76%;
  }
  .input-text {
    font-size: 18px;
    font-family: Poppins-Regular, sans-serif;
    color: black;
  }
  .declaration {
    background-color: white;
    padding: 10px;
    width: 154%;
    font-size: 18px;
  }
  .mobile-pre {
    width: 12%;
  }
  .mobile-post {
    width: 80%;
  }
}

@media screen and (max-width: 768px) {
  .form-groupBlogg {
    width: 100%;
    margin-top: 15%;
  }
  /* .form-group {
    height: 160%;
  } */
  .gender-box {
    width: 94%;
  }
  .mobile-input {
    width: 76%;
  }
  .input-text {
    font-size: 14px;
    font-family: Poppins-Regular, sans-serif;
    color: black;
  }
  .declaration {
    background-color: white;
    padding: 10px;
    width: 154%;
    font-size: 14px;
  }
  .mobile-pre {
    width: 23%;
  }
  .mobile-post {
    width: 65%;
  }
}

.container {
  border: 1px solid black;
  margin: 60px auto;
  width: 1050px;
  height: 700px;
  background-color: white;
  margin-top: 0px;
}

.nav-container {
  width: 1048px;
  height: 55px;
  background-color: #e43927;
  margin-left: -16px;
}
.nav-text {
  color: #e43927;
  margin-left: 20px;
  padding-top: 16px;
  font-size: 20px;
}
.button {
  width: 112px;
  height: 42px;
  color: #fff;
  background: #e43927;
  border-radius: 50px;
  font-size: 0.875rem;
  text-transform: uppercase;
  font-family: Poppins-Regular, sans-serif;
  font-weight: 600;
  cursor: pointer;
  border: 2px solid #e43927;
  transition: 0.5s;
  letter-spacing: 0.2px;
}

.input-border {
  display: block;
  /* width: 100%; */
  height: calc(1.5em + 0.75rem + 2px);
  padding: 0.375rem 0.75rem;
  font-size: 1rem;
  font-weight: 400;
  line-height: 1.5;
  color: #495057;
  background-color: white;
  background-clip: padding-box;
  border: 0px;
  transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
}
.logo-img {
  width: 190px;
  margin-left: 17%;
  margin-top: -6%;
}
