      <div
        class="row col-12 col-lg-12"
        style="margin-top: 7%; margin-left: 8%"
        *ngIf="isShownAfterRegistration"
      >
        <div class="col-6 col-lg-6 dwnld-btn">
          <button
            class="btn mr-3"
            style="
              height: 42px;
              color: #fff;
              background: #e43927;
              border-radius: 50px;
              font-size: 0.875rem;
              text-transform: uppercase;
              font-family: Poppins-Regular, sans-serif;
              font-weight: 600;
              cursor: pointer;
              border: 2px solid #e43927;
              transition: 0.5s;
              letter-spacing: 0.2px;
            "
            (click)="downloadHealthIdCard()"
          >
            Download ABHA ID PDF
          </button>
        </div>
        <div class="col-6 col-lg-6 dwnld-btn">
          <button
            class="btn mr-3"
            style="
              height: 42px;
              color: #fff;
              background: #e43927;
              border-radius: 50px;
              font-size: 0.875rem;
              text-transform: uppercase;
              font-family: Poppins-Regular, sans-serif;
              font-weight: 600;
              cursor: pointer;
              border: 2px solid #e43927;
              transition: 0.5s;
              letter-spacing: 0.2px;
            "
            (click)="downloadHealthIdCardQR()"
          >
            Download ABHA ID PNG
          </button>
        </div>
      </div>

      <div
        class="row col-12 col-lg-12"
        *ngIf="!isShownAfterRegistration"
        style="margin-top: 10%"
      >
        <div class="col-1 col-lg-1 check-box">
          <input
            type="checkbox"
            [readonly]="isShownAfterRegistration"
            [(ngModel)]="isChecked"
            name="check"
            required
          />
        </div>
        <div class="col-11 col-lg-11 terms">
          <p style="font-size: 18px; font-family: Poppins-Regular, sans-serif">
            Accept
            <a (click)="openPopup()" style="text-decoration: underline"
              >Terms & Conditions</a
            >
          </p>

          <div
            class="modal"
            tabindex="-1"
            role="dialog"
            [ngStyle]="{ display: displayStyle }"
          >
            <div class="modal-dialog" role="document" style="max-width: 50%">
              <div
                class="modal-content modal-cntr"
                style="border: 1px solid #e43927"
              >
                <div
                  class="modal-header modal-head"
                  style="
                    background-color: #e43927;
                    margin-top: -20px;
                    margin-left: -20px;
                  "
                >
                  <p style="color: white; font-size: 23px; padding-top: 10px">
                    Terms & Conditions
                  </p>
                  <h1
                    style="color: white; cursor: pointer"
                    (click)="closePopup()"
                  >
                    X
                  </h1>
                </div>
                <div class="modal-body" style="width: 84%">
                  <p
                    style="
                      background-color: white;
                      width: 125%;
                      margin-left: -12px;
                    "
                  >
                    I, hereby declare that I am voluntarily sharing my identity
                    information with National Health Authority (NHA) for the
                    sole purpose of creation of ABHA ID. I understand that my
                    ABHA ID can be used and shared for purposes as may be
                    notified by ABDM (Ayushman Bharat Digital Mission) from time
                    to time including provision of healthcare services. Further,
                    I am aware that my personal identifiable information (Name,
                    Address, Age, Date of Birth, Gender and Photograph) may be
                    made available to the entities working in the National
                    Digital Health Ecosystem (NDHE) which inter alia includes
                    stakeholders and entities such as healthcare professionals
                    (e.g. doctors), facilities (e.g. hospitals, laboratories)
                    and data fiduciaries (e.g. health programmes), which are
                    registered with or linked to the Ayushman Bharat Digital
                    Mission (ABDM), and various processes there under.<span
                      class="error"
                      >*</span
                    >
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div
        class="row col-12 col-lg-12"
        style="margin-top: 10%; margin-left: 1%"
        *ngIf="!isShownAfterRegistration"
      >
        <div class="col-3 col-lg-3 cancel-btn">
          <button class="btn button mr-3" (click)="back()">CANCEL</button>
        </div>
        <div class="col-3 col-lg-3 clear-btn">
          <button class="btn button mr-3" (click)="clearFields()">CLEAR</button>
        </div>
        <div class="col-3 col-lg-3 submit-btn">
          <button
            class="btn button"
            *ngIf="!isShownAfterRegistration"
            [disabled]="!regestrationForm.valid || isShownAfterRegistration"
            (click)="next()"
          >
            SUBMIT
          </button>
          <button
            class="btn button"
            *ngIf="isShownAfterRegistration"
            [disabled]="showPopupButtonFlag"
            (click)="showPopup()"
          >
            SUBMIT
          </button>
        </div>
      </div>
    </form>
  </div>

  <div class="row form-groupBlogg">
    <div class="col-2 col-lg-3"></div>
    <div class="col-4 col-lg-3">
      <p
        style="
          font-family: Poppins-Regular, sans-serif;
          color: black;
          margin-bottom: 0px;
        "
      >
        Powered BY
      </p>
    </div>
    <div class="col-4 col-lg-4" style="margin-top: -1%">
      <img src="assets/logo/logos.png" />
    </div>
  </div>
</div>

<div
  *ngIf="isShowPopup"
  class="sample form-group"
  style="background-color: white; padding-left: 3%"
>
  <nav class="navbar navbar-expand-sm navbar-dark" style="margin-left: -6%">
    <p class="nav-text">Create ABHA ID</p>
  </nav>
  <div class="row col-12 col-lg-12" style="margin-top: 10%">
    <div class="col-2 col-lg-2" style="height: 38px">
      <p
        class="mt-1"
        style="
          font-size: 18px;
          color: black;
          font-family: Poppins-Regular, sans-serif;
        "
      >
        Enter OTP
      </p>
    </div>
    <div class="col-3 col-lg-3">
      <div class="form-group">
        <input
          type="text"
          [(ngModel)]="otp"
          autocomplete="disabled"
          class="input-border input"
          name="otpName"
          style="border-bottom: 1px solid #e43927 !important; width: 56px"
        />
      </div>
    </div>
    <div class="col-2 col-lg-2">
      <label
        style="
          background-color: white;
          text-align: center;
          border: 2px solid black;
          padding: 3px;
        "
        >00:{{ timeLeft }}</label
      >
    </div>
    <div class="col-3 col-lg-3" style="margin-top: -15px">
      <button
        class="btn button mr-3"
        (click)="resendOTP()"
        [disabled]="timeLeft > 0"
      >
        Resend OTP
      </button>
    </div>
  </div>

  <div class="row col-12 col-lg-12" style="margin-top: 10%">
    <div class="col-3 col-lg-3"></div>
    <!-- <div class="col-3 col-lg-3" style="margin-left: -3%">
      <button class="btn button" (click)="backPopup()">BACK</button>
    </div> -->
    <div class="col-3 col-lg-3" style="margin-left: 15%">
      <button class="btn button" (click)="submit()">SUBMIT</button>
    </div>
  </div>

  <div class="row logo">
    <div class="col-2 col-lg-3"></div>
    <div class="col-4 col-lg-3">
      <p
        style="
          font-family: Poppins-Regular, sans-serif;
          color: black;
          margin-bottom: 0px;
        "
      >
        Powered BY
      </p>
    </div>
    <div class="col-4 col-lg-4" style="margin-top: -1%">
      <img src="assets/logo/logos.png" />
    </div>
  </div>
</div>

