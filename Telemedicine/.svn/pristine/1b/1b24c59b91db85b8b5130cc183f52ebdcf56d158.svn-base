.error {
  color: red;
}
[class*="col-"] {
  float: left;
  padding: 15px;
}
/* For desktop: */
.col-1 {
  width: 8.33%;
}
.col-2 {
  width: 16.66%;
}
.col-3 {
  width: 25%;
}
.col-4 {
  width: 33.33%;
}
.col-5 {
  width: 41.66%;
}
.col-6 {
  width: 50%;
}
.col-7 {
  width: 58.33%;
}
.col-8 {
  width: 66.66%;
}
.col-9 {
  width: 75%;
}
.col-10 {
  width: 83.33%;
}
.col-11 {
  width: 91.66%;
}
.col-12 {
  width: 100%;
}

@media only screen and (min-width: 768px) {
  /* For mobile phones: */
  [class*="col-"] {
    width: 100%;
    height: 100%;
  }
  .sample {
    border: 1px solid black;
    margin-left: 25%;
    width: 50%;
  }
  .resend-btn {
    width: 33.33%;
  }
}

@media screen and (min-width: 768px) {
  .form-groupBlogg {
    width: 100%;
    margin-top: 4%;
  }
  .form-group {
    height: 75%;
  }
}

@media screen and (max-width: 768px) {
  .form-groupBlogg {
    width: 100%;
    margin-top: 80%;
  }
  .form-group {
    height: 103%;
  }
}

.container {
  border: 1px solid black;
  margin: 60px auto;
  width: 1050px;
  height: 720px;
  background-color: white;
  margin-top: 0px;
}

.nav-container {
  width: 1048px;
  height: 55px;
  background-color: #e43927;
  margin-left: -16px;
}
.nav-text {
  color: #e43927;
  margin-left: 20px;
  padding-top: 16px;
  font-size: 20px;
}
.main-text {
  font-size: 22px;
  font-family: "Poppins-Regular, sans-serif";
  padding-left: 70px;
  padding-top: 70px;
}
.text {
  font-size: 20px;
  font-family: "Poppins-Regular, sans-serif";
  padding-left: 70px;
}
.input-box {
  display: block;
  width: 100%;
  height: calc(1.5em + 0.75rem + 2px);
  padding: 0.375rem 0.75rem;
  font-size: 1rem;
  font-weight: 400;
  line-height: 1.5;
  color: #495057;
  background-color: #fff;
  background-clip: padding-box;
  border: 0px;
  /* border-radius: 0.25rem; */
  transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
}

.button {
  width: 112px;
  height: 42px;
  color: #fff;
  background: #e43927;
  border-radius: 50px;
  font-size: 0.875rem;
  text-transform: uppercase;
  font-family: Poppins-Regular, sans-serif;
  font-weight: 600;
  cursor: pointer;
  border: 2px solid #e43927;
  transition: 0.5s;
  letter-spacing: 0.2px;
}
.logo-img {
  width: 190px;
  margin-left: 17%;
  margin-top: -6%;
}
