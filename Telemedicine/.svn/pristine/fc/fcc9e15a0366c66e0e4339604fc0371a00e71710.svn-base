<mat-table [dataSource]="dataSource" class="clinic-table">
    <ng-container matColumnDef="date">
        <mat-header-cell *matHeaderCellDef> <b> Date </b> </mat-header-cell>
        <mat-cell *matCellDef="let element"> {{element.date_added}}</mat-cell>
    </ng-container>

    <ng-container matColumnDef="flag">
        <mat-header-cell *matHeaderCellDef> <b> Red Flag </b> </mat-header-cell>
        <mat-cell *matCellDef="let element"> {{element.red_flag}}</mat-cell>
    </ng-container>

    <ng-container matColumnDef="link">
        <mat-header-cell *matHeaderCellDef> <b> Link </b> </mat-header-cell>
        <mat-cell *matCellDef="let element">
            <img src="assets/img/note.svg" role="button" class="w-auto m-0" (click)="copyUrl(element.red_flag)" />
        </mat-cell>
    </ng-container>

    <mat-header-row *matHeaderRowDef="displayedColumns"></mat-header-row>
    <mat-row *matRowDef="let row; columns: displayedColumns;">
    </mat-row>
</mat-table>