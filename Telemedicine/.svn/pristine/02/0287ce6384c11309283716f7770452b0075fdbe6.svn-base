<app-breadcrumb [showHomePageOptions]="true"></app-breadcrumb>
<ngx-spinner>
    <p class="loading">Loading...</p>
</ngx-spinner>
<div class="container-fluid docReg pl-5">
    <div class="row"></div>
    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
        <div class="row">
            <div class="col-sm-9 col-md-9 col-lg-9 col-xl-9 mt-3">
                <div class="row">
                    <!-- Headings -->
                    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <div class="col-12">
                            <h2 class="mb-1 pl-1"> 
                                <span>
                                   <b> {{'lblRegisterDoctor' | l10n}} </b>
                                </span>
                            </h2>
                            <p>Note: Please upload at least one document.</p>
                        </div>
                    </div>

                    <!-- Card design START -->
                    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12">
                        <div class="col-12 pb-3">
                            <div class="bgShadowStyle borderRadius15">
                                <mat-card class="borderRadius15">
                                    <form [formGroup]="form">
                                        <div class="row">
                                            <div class="col-sm-12 col-md-10 col-lg-10 col-xl-10">
                                                <div class="row">
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                        <mat-form-field class="example-full-width w-100 " appearance="outline" [ngbTooltip]="fullnametool">
                                                            <mat-label class="labelSize14">
                                                                <b>{{'lblFullName' | l10n}}</b>
                                                            </mat-label>
                                                              <input matInput maxlength="30" formControlName="doctorName" placeholder="FirstName MiddleName  LastName"  autocomplete="off" required>
                                                              <mat-error *ngIf="form.get('doctorName').errors && (form.get('doctorName').touched || form.get('doctorName').dirty)">
                                                                <span *ngIf="form.get('doctorName').errors.required">{{'errFullNameRequired' | l10n}}</span>
                                                                <span *ngIf="form.get('doctorName').errors.pattern">{{'errFullNamePattern' | l10n}}</span>
                                                                <span *ngIf="form.get('doctorName').errors.maxlength">{{'errFullNameMaxLength' | l10n}}</span>
                                                            </mat-error>
                                                            </mat-form-field>
                                                    </div>
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                        <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                            <mat-label class="labelSize14">
                                                                <b>{{'lblSMCNo' | l10n}}</b>
                                                            </mat-label>
                                                              <input matInput maxlength="25" formControlName="smcNo" autocomplete="off" placeholder="Enter {{'lblSMCNo' | l10n}}" required> 
                                                              <mat-error *ngIf="form.get('smcNo').errors && (form.get('smcNo').touched || form.get('smcNo').dirty)">
                                                                <span *ngIf="form.get('smcNo').errors.required">{{'errSMCrequired' | l10n}}</span>
                                                                <span *ngIf="form.get('smcNo').errors.minlength">{{'errMCIMinLength' | l10n}}</span>
                                                                <span *ngIf="form.get('smcNo').errors.maxlength">{{'errSMCMaxLength' | l10n}}</span>

                                                                <!-- <span *ngIf="form.get('smcNo').errors.pattern">{{'errSMCPattern' | l10n}}</span> -->
                                                            </mat-error>
                                                        </mat-form-field>
                                                    </div>
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4"> 
                                                        <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                            <mat-label class="labelSize14">
                                                                <b>{{'lblMICNo' | l10n}}</b>
                                                            </mat-label>
                                                              <input matInput maxlength="25" formControlName="micNo" autocomplete="off" placeholder="Enter {{'lblMICNo' | l10n}}" required>
                                                              <mat-error *ngIf="form.get('micNo').errors && (form.get('micNo').touched || form.get('micNo').dirty)">
                                                                <span *ngIf="form.get('micNo').errors.required">{{'errMCIRequired' | l10n}}</span>
                                                                <span *ngIf="form.get('micNo').errors.minlength">{{'errMCIMinLength' | l10n}}</span>
                                                                <span *ngIf="form.get('micNo').errors.maxlength">{{'errMCIMaxLength' | l10n}}</span>                                                              

                                                                <!-- <span *ngIf="form.get('micNo').errors.pattern">{{'errMCIPattern' | l10n}}</span> -->
                                                            </mat-error>
                                                        </mat-form-field>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                        <mat-form-field  class="example-full-width w-100 " appearance="outline">
                                                            <mat-label class="labelSize14"><b>{{'lblSpecialization' | l10n}}</b></mat-label>
                                                            <mat-select formControlName="specialization" [disabled]="disableSelect.value"required>
                                                                <mat-option>{{'lblSpecialization' | l10n}}</mat-option>
                                                                <mat-option *ngFor = "let specialization of specializationList" [value]="specialization.masterValue">{{specialization.masterValue}}</mat-option>
                                                            </mat-select>
                                                            <mat-error *ngIf="form.get('specialization').errors && (form.get('specialization').touched || form.get('specialization').dirty)">
                                                                <span *ngIf="form.get('specialization').errors.required">{{'errSpecilasationRequired' | l10n}}</span>
                                                            </mat-error>
                                                        </mat-form-field>
                                                    </div>
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                        <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                            <mat-label class="labelSize14">
                                                                <b>{{'lblConsultationFee' | l10n}}</b>
                                                            </mat-label>
                                                              <input matInput type="number" numberOnly length="15" formControlName="consultationFee" autocomplete="off" placeholder="Enter {{'lblConsultationFee' | l10n}}" required>
                                                              <mat-error *ngIf="form.get('consultationFee').errors && (form.get('consultationFee').touched || form.get('consultationFee').dirty)">
                                                                <span *ngIf="form.get('consultationFee').errors.required">{{'errConsultantRequired' | l10n}}</span>
                                                                <span *ngIf="form.get('consultationFee').errors.maxlength">{{'errConsultantMaxLength' | l10n}}</span>
                                                                <span *ngIf="form.get('consultationFee').errors.pattern">{{'errConsultantPattern' | l10n}}</span>
                                                            </mat-error>
                                                        </mat-form-field>
                                                    </div>
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                        <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                            <mat-label class="labelSize14">
                                                                <b>{{'lblMobileNumber' | l10n}}</b>
                                                            </mat-label>
                                                              <input matInput type="number" numberOnly length="10" formControlName="mobileNo" placeholder="Enter {{'lblMobileNumber' | l10n}}" autocomplete="off" required>
                                                              <mat-error *ngIf="form.get('mobileNo').errors && (form.get('mobileNo').touched || form.get('mobileNo').dirty)">
                                                                <span *ngIf="form.get('mobileNo').errors.required">{{'errMobileRequired' | l10n}}</span> 
                                                                <span *ngIf="form.get('mobileNo').errors.maxlength">{{'errMobileMaxLength' | l10n}}</span>
                                                                <span *ngIf="form.get('mobileNo').errors.pattern">{{'errMobilePattern' | l10n}}</span>
                                                            </mat-error>
                                                        </mat-form-field>
                                                    </div>
                                                </div>
                                                <div class="row mt-1">
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                        <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                            <mat-label class="labelSize14">
                                                                <b>Address1</b>
                                                            </mat-label>
                                                              <input matInput maxlength="75" formControlName="address1" autocomplete="off" placeholder="Enter Address1" required>
                                                              <mat-error *ngIf="form.get('address1').errors && (form.get('address1').touched || form.get('address1').dirty)">
                                                                <span *ngIf="form.get('address1').errors.required">{{'errAddressRequired' | l10n}}</span>
                                                                <span *ngIf="form.get('address1').errors.maxlength">{{'errAddressMaxLength' | l10n}}</span>
                                                                <span *ngIf="form.get('address1').errors.pattern">{{'errAddressPattern' | l10n}}</span>
                                                            </mat-error>
                                                        </mat-form-field>
                                                    </div>
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                        <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                            <mat-label class="labelSize14">
                                                                <b>Address2</b>
                                                            </mat-label>
                                                              <input matInput maxlength="75" formControlName="address2" autocomplete="off" placeholder="Enter Address2">
                                                        </mat-form-field>
                                                    </div>
                                                    <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                        <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                            <mat-label class="labelSize14">
                                                                <b>Address3</b>
                                                            </mat-label>
                                                              <input matInput maxlength="75" formControlName="address3" autocomplete="off" placeholder="Enter Address3">
                                                        </mat-form-field>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-12 col-md-2 col-lg-2 col-xl-2">
                                                <div class="row">
                                                    <span>
                                                        <b>{{'lblUploadPhoto' | l10n}}</b>&nbsp;
                                                        <label class="switch">
                                                            <input type="checkbox" (click) = "onClickSwitch()" [checked] = 'isSwitcheChecked'>
                                                            <span class="slider round"></span> 
                                                        </label>
                                                    </span>
                                                    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12" *ngIf ="!isSwitcheChecked">
                                                        <div class="profile-wrapper"> 
                                                            <img src="{{uploadedImage}}" id="ProfilePhoto" alt="Avatar" class="rounded-circle">
                                                        </div>
                                                    </div> 
                                                    
                                                    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12" *ngIf="isSwitcheChecked">
                                                        <div>
                                                            <webcam
                                                            [height]="75"  
                                                            [width]="100"  
                                                            [trigger]="triggerObservable" 
                                                            (initError)="handleInitError($event)"
                                                            (imageCapture)="handleImage($event)"
                                                            > 
                                                            </webcam> 
                                                            <!-- &nbsp;  -->
                                                            <br/>
                                                            <button class="actionBtn btn btn-info btn-sm" (click)="triggerSnapshot()">{{'lblTakepicture' | l10n}}</button> 
                                                        </div>
                                                    </div> 
                                                    <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 ml-1">
                                                        <!-- <label class="uploadPhotoText text-muted mt-1">{{'lblUploadPhoto' | l10n}}</label> &nbsp;&nbsp; -->
                                                        
                                                        <br/>
                                                        <div class="ml-2">
                                                            <input type="file" id="browse" formControlName="profilePhoto" class="mt-1" (change)="uploadProfilePhoto($event)" hidden>
                                                            <button type="button" *ngIf="!isSwitcheChecked" class="btn btnColor btn-sm" (click)="browse()">
                                                                Browse
                                                            </button>               
                                                        </div>
                                                    </div> 
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row mt-1">
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field  class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14"><b>{{'lblState' | l10n}}</b></mat-label>
                                                    <mat-select formControlName="state" [disabled]="disableSelect.value" (selectionChange)="onSelectState($event.value)" required>
                                                        <mat-option>{{'lblState' | l10n}}</mat-option>
                                                        <mat-option *ngFor = "let state of states" [value]="state.stateName">{{state.stateName}}</mat-option>
                                                        <!-- <mat-option value="Maharashtra">Maharashtra</mat-option>
                                                        <mat-option value="Panjab">Panjab</mat-option> -->
                                                    </mat-select>
                                                    <mat-error *ngIf="form.get('state').errors && (form.get('state').touched || form.get('state').dirty)">
                                                        <span *ngIf="form.get('state').errors.required">{{'errStateRequired' | l10n}}</span>
                                                    </mat-error>
                                                </mat-form-field>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field  class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14"><b>{{'lblCity' | l10n}}</b></mat-label>
                                                    <mat-select formControlName="city" [disabled]="disableSelect.value" required>
                                                        <mat-option>{{'lblCity' | l10n}}</mat-option>
                                                        <mat-option *ngFor = "let city of cities" [value]="city.cityName">{{city.cityName}}</mat-option>
                                                        <!-- <mat-option value="Hinjewadi">Hinjewadi</mat-option>
                                                        <mat-option value="Baner">Baner</mat-option> -->
                                                    </mat-select>
                                                    <mat-error *ngIf="form.get('city').errors && (form.get('city').touched || form.get('city').dirty)">
                                                        <span *ngIf="form.get('city').errors.required">{{'errCityRequired' | l10n}}</span>
                                                    </mat-error>
                                                </mat-form-field>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14">
                                                        <b>{{'lblLocality' | l10n}}</b>
                                                    </mat-label>
                                                    <input matInput formControlName="locality" placeholder="Enter {{'lblEnterLocality' | l10n}}" autocomplete="off">
                                                    <!-- <mat-error *ngIf="form.get('locality').errors && (form.get('locality').touched || form.get('locality').dirty)">
                                                        <span *ngIf="form.get('email').errors.required">{{'errEmailRequired' | l10n}}</span>
                                                        <span *ngIf="form.get('email').errors.email">{{'errEmailPattern' | l10n}}</span>
                                                        <span *ngIf="!form.get('email').errors.email && form.get('email').errors.maxlength">{{'errEmailMaxlength' | l10n}}</span>
                                                    </mat-error> -->
                                                </mat-form-field>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field  class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14"><b>Gender</b></mat-label>
                                                    <mat-select formControlName="gender" [disabled]="disableSelect.value" required>
                                                        <mat-option *ngFor = "let gender of genderList" [value]="gender.masterValue">{{gender.masterValue}}</mat-option>
                                                        <!-- <mat-option value="Female">Female</mat-option> -->
                                                    </mat-select> 
                                                    <mat-error *ngIf="form.get('gender').errors && (form.get('gender').touched || form.get('gender').dirty)">
                                                        <span *ngIf="form.get('gender').errors.required">{{'errGenderRequired' | l10n}}</span>
                                                    </mat-error>
                                                </mat-form-field>
                                            </div>
                                        </div>
                                        <div class="row mt-1">
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3" >
                                                <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14">
                                                        <b>{{'lblEmailId' | l10n}}</b>
                                                    </mat-label>
                                                    <input matInput formControlName="email" placeholder="Enter {{'lblEmailId' | l10n}}" autocomplete="off" required>
                                                    <mat-error *ngIf="form.get('email').errors && (form.get('email').touched || form.get('email').dirty)">
                                                        <span *ngIf="form.get('email').errors.required">{{'errEmailRequired' | l10n}}</span>
                                                        <span *ngIf="form.get('email').errors.email">{{'errEmailPattern' | l10n}}</span>
                                                        <span *ngIf="!form.get('email').errors.email && form.get('email').errors.maxlength">{{'errEmailMaxlength' | l10n}}</span>
                                                    </mat-error>
                                                </mat-form-field>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14">
                                                        <b>{{'lblUserName' | l10n}}</b>
                                                    </mat-label>
                                                    <input matInput formControlName="userName"  autocomplete="off" minlength="8" maxlength="25" placeholder="Enter {{'lblUserName' | l10n}}" required>
                                                    <mat-error *ngIf="form.get('userName').errors && (form.get('userName').touched || form.get('userName').dirty)">
                                                        <span *ngIf="form.get('userName').errors.required">{{'errUserNameRequired' | l10n}}</span>
                                                        <span *ngIf="form.get('userName').errors.maxlength">{{'errUserNameMaxLength' | l10n}}</span>
                                                        <span *ngIf="form.get('userName').errors.minlength">{{'errUserNameMinLength' | l10n}}</span>
                                                        <span *ngIf="form.get('userName').errors.pattern">{{'errUserNamePattern' | l10n}}</span>
                                                    </mat-error> 
                                                    </mat-form-field>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field class="example-full-width w-100 " appearance="outline" [ngbTooltip]="template">
                                                    <mat-label class="labelSize14">
                                                        <b>{{'lblPassword' | l10n}}</b>
                                                    </mat-label>
                                                      <input [type]="hidePassword? 'password' : 'text'" matInput formControlName="password" autocomplete="off" minlength="8" maxlength="16" placeholder="Enter {{'lblPassword' | l10n}}" autocomplete="new-password" required>
                                                      <i matSuffix *ngIf="!hidePassword" (click)="hidePassword = !hidePassword" class="fa fa-eye eye-icon"></i> 
                                                      <i matSuffix *ngIf="hidePassword" (click)="hidePassword = !hidePassword" class="fa fa-eye-slash eye-icon"></i> 
                                                      <!-- <mat-icon matSuffix class="cursorHand" (click)="hidePassword = !hidePassword"> {{hidePassword? 'visibility_off' : 'visibility'}} </mat-icon> -->
                                                      <mat-error *ngIf="form.get('password').errors && (form.get('password').touched || form.get('password').dirty)">
                                                        <span *ngIf="form.get('password').errors.required">{{'errpasswordRequired' | l10n}}</span>&nbsp;
                                                        <span *ngIf="form.get('password').errors.maxlength">{{'lblPasswordCondition2' | l10n}}</span>&nbsp;
                                                        <span *ngIf="form.get('password').errors.minlength">{{'errPasswordMinLength' | l10n}}</span>&nbsp;
                                                        <span *ngIf="form.get('password').value && form.get('password').invalid">{{'lblPasswordMatch' | l10n}}</span>                                                       
                                                    </mat-error> 
                                                </mat-form-field>
                
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field class="example-full-width w-100 " appearance="outline" [ngbTooltip]="template">
                                                    <mat-label class="labelSize14">
                                                        <b>{{'lblReEnterPassword' | l10n}}</b>
                                                    </mat-label>
                                                      <input [type]="hideReEnterPassword? 'password' : 'text'" matInput formControlName="reEnterPassword"  autocomplete="off" placeholder="Enter {{'lblReEnterPassword' | l10n}}"  (input)="onPasswordInput()" required>
                                                      <i  matSuffix *ngIf="!hideReEnterPassword"  (click)="hideReEnterPassword = !hideReEnterPassword" class="fa fa-eye eye-icon"></i> 
                                                      <i matSuffix *ngIf="hideReEnterPassword" (click)="hideReEnterPassword = !hideReEnterPassword" class="fa fa-eye-slash eye-icon"></i> 
                                                      <!-- <mat-icon matSuffix class="cursorHand" (click)="hideReEnterPassword = !hideReEnterPassword"> {{hideReEnterPassword? 'visibility_off' : 'visibility'}} </mat-icon> -->
                                                      <mat-error *ngIf="form.get('reEnterPassword').errors && (form.get('reEnterPassword').touched || form.get('reEnterPassword').dirty)">
                                                        <span *ngIf="form.get('reEnterPassword').errors.required">{{'errpasswordRequired' | l10n}}</span>&nbsp;
                                                        <span *ngIf="form.get('reEnterPassword').errors.maxlength">{{'lblPasswordCondition2' | l10n}}</span>&nbsp;
                                                        <span *ngIf="form.get('reEnterPassword').errors.minlength">{{'errPasswordMinLength' | l10n}}</span>&nbsp;
                                                        <span *ngIf="form.get('reEnterPassword').invalid && !form.get('reEnterPassword').hasError('required')">{{'lblEnterCorrectPass' | l10n}}</span>
                                                    </mat-error> 
                                                    </mat-form-field>
                                            </div>
                                        </div>
                                        <div class="row mt-1">
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field class="example-full-width w-100" appearance="outline">
                                                    <mat-label class="labelSize14"><b>{{ "lblAssociation" | l10n }} </b></mat-label>
                                                    <mat-select formControlName="associationName">
                                                        <mat-option *ngFor="let association of associationList" [value]="association.associationName">{{association.associationName}}
                                                        </mat-option>
                                                    </mat-select>
                                                </mat-form-field>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field class="example-full-width w-100" appearance="outline">
                                                    <mat-label class="labelSize14"><b>{{ "lblAssociationNo" | l10n }}</b> </mat-label>
                                                    <input matInput type="text" maxlength="30" formControlName="associationNumber" />
                                                    <mat-error
                                                        *ngIf="form.get('associationNumber').errors && (form.get('associationNumber').touched || form.get('associationNumber').dirty)">
                                                        <span *ngIf="form.get('associationNumber').errors.required">{{"errAssociationNoRequired" | l10n}}</span>
                                                    </mat-error>
                                                </mat-form-field>
                                            </div>
                                        </div>
                                        <div class="row mt-2">
                                            <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14">
                                                        <b>{{'preAssessmentLink' | l10n}}</b>
                                                    </mat-label>
                                                      <input matInput maxlength="100" formControlName="pre_assessment_link" autocomplete="off" placeholder="Enter {{'preAssessmentLink' | l10n}}">
                                                </mat-form-field>
                                            </div>
                                        </div>
                                        <div class="row mt-2">
                                            <div class="col-sm-4 col-md-4 col-lg-4 col-xl-4">
                                                <mat-form-field  class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14"><b>Document Type</b></mat-label>
                                                    <mat-select formControlName="documentType" [disabled]="disableSelect.value" required>
                                                        <mat-option>Document Type</mat-option>
                                                        <mat-option *ngFor = "let type of documentTypes" [value]="type.masterValue">{{type.masterValue}}</mat-option>
                                                    </mat-select>
                                                    <mat-error *ngIf="form.get('documentType').errors && (form.get('documentType').touched || form.get('documentType').dirty)">
                                                        <span *ngIf="form.get('documentType').errors.required">{{'errdocRequired' | l10n}}</span>
                                                    </mat-error>
                                                </mat-form-field>
                                                <label>Note:Upload atleast 1 document</label>
                                            </div>
                                            <div class="col-sm-2 col-md-2 col-lg-2 col-xl-2">
                                                <div class="col-12">
                                                    <!-- <label class="labelSize">Upload Documents</label> -->
                                                    <!-- <small class="text-muted"> ( Document should be in a PDF, JPG, & PNG format )</small> -->
                                                    <!-- <br/> -->
                                                    <input type="file" id="browseFile" class="mt-2" formControlName="filesToUpload" (change)="showSelectedFiles($event)" multiple hidden>
                                                    <button class="btn btnColor btn-sm mt-2" (click)="browseFile()">
                                                        Browse
                                                    </button>


                                                    <!-- PROGRESS BAR  *ngIf="progress"  -->
                                                    <!-- <mat-progress-bar class="mt-2" mode="determinate" value="{{progress}}"></mat-progress-bar> -->
                                                </div>
                                                
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <div class="row">
                                                    <div class="col-8 mt-3">
                                                        <img src="{{captcha}}" alt="Cpacha_Image" width="100%">
                                                    </div>
                                                    <div class="col-2 mt-3">
                                                        <button type="button" (click)="refreshCpacha()" data-toggle="tooltip" title="Refresh" class="btn p-0">
                                                            <i class="fa fa-refresh" aria-hidden="true"></i>
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3">
                                                <mat-form-field class="example-full-width w-100 " appearance="outline">
                                                    <mat-label class="labelSize14">
                                                        <b>{{'lblEnterCpacha' | l10n}}</b>
                                                    </mat-label>
                                                      <input matInput formControlName="captcha" autocomplete="off" placeholder="Enter {{'lblEnterCpacha' | l10n}}" required>
                                                      <mat-error *ngIf="form.get('captcha').errors && (form.get('captcha').touched || form.get('captcha').dirty)">
                                                        <span *ngIf="form.get('captcha').errors.required">{{'errCaptchaRequired' | l10n}}</span>
                                                    </mat-error>
                                                </mat-form-field>
                                            </div>
                                        </div>
                                        <div class="field mb-3" style="text-align: center;">
                                            <div class="control" controlErrorContainer>
                                              <label class="checkbox">
                                              <input type="checkbox" formControlName="terms"  required>
                                            </label>
                                             I agree to the 
                                             <button mat-button (click)="openDialogTC(content)" style="padding: 0 2px;">
                                                <span style="text-decoration: underline  #007bff; color:#007bff;">Terms and Conditions</span>
                                            </button>
                                              <mat-error *ngIf="form.get('terms').errors && (form.get('terms').touched || form.get('terms').dirty)">
                                                <span *ngIf="form.get('terms').errors.required">{{'errTermsRequired' | l10n}}</span>
                                            </mat-error>
                                            </div>
                                          </div>
                                        <!-- Show uploaded files -->
                                        <div class="row mt-2">
                                            <div class="col-4 p-2" *ngFor="let file of uploadedFileNames; let i=index">
                                                <ul class="fileUpload p-0 mb-2">
                                                    <li class="border-bottom">
                                                        {{file.name}}
                                                        <button type="button" class="btn btn-default btn-sm p-0 float-right" (click)="removeFiles(i)">
                                                            <i class="fa fa-trash-o text-danger" aria-hidden="true"></i>
                                                        </button>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                        <div class="row mt-1">
                                            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-12 text-center">
                                                <button class="btn btnColor mt-2" mat-raised-button (click)="verifyCaptcha()">Submit</button>&nbsp;
                                                <button class="btn btnColor mt-2" mat-raised-button (click)="reset()">Clear</button>&nbsp;
                                                <button class="btn btnColor mt-2" mat-raised-button (click)="onClickCancel()">Cancel</button>
                                            </div>
                                        </div>
                                    </form>
                                </mat-card>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Personal details design -->
            <div class="col-sm-3 col-md-3 col-lg-3 col-xl-3 mt-5">
                <div class="personalDetailsList bgShadowStyle bg-info">
                    <div class="col-12 border-bottom text-center pt-3">
                        <h2>
                            Personal Details
                        </h2>
                    </div>
                    <div class="col-12 pt-2 pb-1">
                        <ul class="list pl-0">
                            <li *ngFor="let item of personalDataList;" class="pt-1 pb-1">  
                                <i *ngIf="item.stepValue" class="fa fa-circle" aria-hidden="true"></i>
                                <i *ngIf="!item.stepValue" class="fa fa-circle-o" aria-hidden="true"></i>
                                <small class="pl-1"> {{item.stepName}} : </small>
                                <small class="pl-1"> {{item.stepValue}} </small>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>  
        </div>
</div>
<!-- Model -->
