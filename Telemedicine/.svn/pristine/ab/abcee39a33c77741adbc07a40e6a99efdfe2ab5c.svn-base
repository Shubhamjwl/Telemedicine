.card{
    background-color: initial !important;
    margin: 5px 10px;
}
.cardStyle {
    border: 1px solid #fd7b7b;
    border-radius: 12px;
    // width: 200px;
    // min-width: 200px;
    //float: left;
    .card-body {
        .card-text {
            .fa {
                font-size: 20px !important;
            }
        }
    }
}
.cardStyle.disabled{
    opacity: 0.5;
    cursor: not-allowed;
    pointer-events: none;
}
.cardStyle:hover {
    background: #ffffff !important;
    // margin-top: -1px !important;
}
.modal-body{
    label{
        display: inline-block;
        text-align: right;
        width: 30%;
        margin-right: 10px;
    }
}
.model-wrapper{
    position: relative;
    .model-loader{
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        // opacity: 0.8;
        background: #63a4ec40;
        pointer-events: none;
        cursor: not-allowed;
        .loader-wrapper{
            position: relative;
            .loader{
                position: absolute;
                top: 135px;
                left: 45%;
            }
        }
    }
}

.input-Upper{
    text-transform: uppercase;
}
