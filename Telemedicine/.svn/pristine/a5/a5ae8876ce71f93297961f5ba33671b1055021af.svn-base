@media only screen and (min-width: 768px) {
  /* For mobile phones: */
  [class*="col-"] {
    width: 100%;
  }
  .sample {
    border: 1px solid black;
    margin-left: 25%;
    width: 50%;
  }
}
@media screen and (min-width: 768px) {
  .form-groupBlogg {
    width: 100%;
    margin-top: 17%;
  }
  .form-group {
    height: 80%;
  }
  .modal-cntr {
    width: 75%;
  }
  .proceed-btn {
    width: 43%;
  }
}

@media screen and (max-width: 768px) {
  .form-groupBlogg {
    width: 100%;
    margin-top: 100%;
  }
  .form-group {
    height: 100%;
  }
  .modal-cntr {
    width: 185%;
  }
  .proceed-btn {
    width: 80%;
  }
}
